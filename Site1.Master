            .{
    width: 100%;
    height: 100%;
    overflow-x: hidden;
    overflow-y: scroll;
    padding: 0;
    margin: 0;
}

body {
    margin: 0px auto;
    width: 100%;
    background-color: #d8d3cd;
}
.rc_nav {
    overflow: hidden;
    background-color: black;
    text-align: center;
    z-index: 6;
    position: -webkit-sticky;
    font-family: cursive;
}

    .rc_nav a {
        display: inline-block;
        margin-right: -4px;
        color: orange;
        padding: 10px 50px;
        text-decoration: none;
        font-size: 18px;
        -webkit-transition: background 0.3s linear;
        -moz-transition: background 0.3s linear;
        -ms-transition: background 0.3s linear;
        -o-transition: background 0.3s linear;
        transition: background 0.3s linear;
        z-index: 9;
    }

        .rc_nav a:hover {
            background-color: #52575d;
            color: white;
        }

        .rc_nav a:span {
            position: absolute;
            transition: all 0.4s;
        }

span {
    transform: translate(0);
}

.rc_nav .icon {
    display: inline-flex;
}

.rc_content {
    text-align: center;
    padding-left: 14px;
    margin-top: 100px;
    color: black;
}
* {
    font-family: Open Sans;
}

.footer {
    margin-top: 100px;
